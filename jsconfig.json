{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      },
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "lib": ["dom", "es6"],
      "allowJs": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true
    },
    "exclude": ["node_modules", ".next"]
  }
  